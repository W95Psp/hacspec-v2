module Loops.While_loops
#set-options "--fuel 0 --ifuel 1 --z3rlimit 15"
open Core
open FStar.Mul

let f (_: Prims.unit) : u8 =
  let x:u8 = 0uy in
  let x:u8 =
    Rust_primitives.f_while_loop (fun x ->
          let x:u8 = x in
          x <. 10uy <: bool)
      x
      (fun x ->
          let x:u8 = x in
          let x:u8 = x +! 3uy in
          x)
  in
  x +! 12uy
